synergyc --enable-crypto mbpw > $HOME/synergy.log
xset r rate 200 20
setxkbmap -option altwin:swap_alt_win
setxkbmap -option caps:ctrl_modifier
xcape -e '#66=Escape'
xcape -e 'Shift_L=Shift_L|9'
xcape -e 'Shift_R=Shift_L|0'
xrandr --output HDMI1 --auto --output HDMI2 --auto --left-of HDMI1
exec awesome
